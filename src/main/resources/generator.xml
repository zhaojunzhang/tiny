<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC
        "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>


    <!--mysql 连接数据库jar 这里选择自己本地位置-->
    <!-- !!!! Driver Class Path !!!! -->
    <classPathEntry location="/Users/admin/.m2/repository/mysql/mysql-connector-java/8.0.11/mysql-connector-java-8.0.11.jar"/>

    <context id="context" targetRuntime="MyBatis3">
        <commentGenerator>
            <!-- 是否去除自动生成的注释 true：是 ： false:否 -->
            <property name="suppressAllComments" value="false"/>
            <property name="suppressDate" value="true"/>
        </commentGenerator>

        <!--数据库连接的信息：驱动类、连接地址、用户名、密码 -->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://localhost:3306/tiny"
                        userId="root"
                        password="admin123"/>

        <!-- 默认false，把JDBC DECIMAL 和 NUMERIC 类型解析为 Integer，为 true时把JDBC DECIMAL 和
                 NUMERIC 类型解析为java.math.BigDecimal -->
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>

        <!-- targetProject:生成PO类 (实体类)的位置 -->
        <javaModelGenerator targetPackage="com.tiny.entity"
                            targetProject="/Users/admin/IdeaProjects/tiny/src/main/java/">
            <!-- enableSubPackages:是否让schema作为包的后缀 -->
            <property name="enableSubPackages" value="false"/>
            <!-- 从数据库返回的值被清理前后的空格 -->
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>

        <!-- targetProject:mapper映射文件生成的位置
          如果maven工程只是单独的一个工程，targetProject="src/main/java"
          如果maven工程是分模块的工程，targetProject="所属模块的名称"，例如：
          targetProject="ecps-manager-mapper"，下同-->
        <sqlMapGenerator targetPackage="mapper"
                         targetProject="/Users/admin/IdeaProjects/tiny/src/main/resources/">
            <!-- enableSubPackages:是否让schema作为包的后缀 -->
            <property name="enableSubPackages" value="false"/>
        </sqlMapGenerator>

        <!-- targetPackage：mapper接口生成的位置 -->
        <javaClientGenerator targetPackage="com.tiny.mapper"
                             targetProject="/Users/admin/IdeaProjects/tiny/src/main/java/" type="XMLMAPPER">
            <!-- enableSubPackages:是否让schema作为包的后缀 -->
            <property name="enableSubPackages" value="false"/>
        </javaClientGenerator>

        <!-- !!!! Table Configurations !!!! -->
        <!-- 指定数据库表 -->
        <!--<table tableName="teacher_message_relational" domainObjectName="TeacherMessageRelational"/>-->
        <!--tableName是表名， domainObjectName是实体类的名-->
        <table tableName="admin" domainObjectName="adminUser1">
        </table>
        <!--<table tableName="user" domainObjectName="User" />-->
        <!--<table tableName="teacher_downgrade_lock" domainObjectName="TeacherDowngradeLock"/>-->
        <!--<table tableName="cms_element_attr" domainObjectName="ElementAttr"/>-->
        <!--<table tableName="teacher_token" domainObjectName="TeacherToken"/>-->
        <!--<table tableName="operation_log" domainObjectName="OperationLog"/>-->


    </context>

    <!--java -jar /Users/admin/Desktop/mybatis-generator-core-1.3.5/lib/mybatis-generator-core-1.3.5.jar -configfile /Users/admin/IdeaProjects/tiny/src/main/resources/generator.xml -overwrite-->

</generatorConfiguration>